{
  "expo": {
    "name": "goblinapp",
    "slug": "goblinapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "scheme": "goblinapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashImg.png",
      "resizeMode": "contain",
      "backgroundColor": "#5DAED6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eazzzzzzy.goblinapp"
    },
    "android": {
      "package": "com.eazzzzzzy.goblinapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c0838412-c03f-4872-9997-be961e6785f7"
      }
    },
    "owner": "eazzzzzzy",
    "plugins": [
      "expo-router",
      "expo-asset"
    ]
  }
}
